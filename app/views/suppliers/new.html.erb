<h1>Cadastrar novo fornecedor</h1>
<% if @supplier.errors.any? %>
  <ul>
    <% @supplier.errors.each do |e| %>
      <li><%= e.full_message %></li>
    <% end %>
  </ul>
<% end %>

<%= form_with(model: @supplier) do |f| %>
  <div>
    <%= f.label :corp_name %>
    <%= f.text_field :corp_name %>
  </div>
  <div>
    <%= f.label :brand_name %>
    <%= f.text_field :brand_name %>
  </div>
  <div>
    <%= f.label :city %>
    <%= f.text_field :city %>
  </div>
  <div>
    <%= f.label :state %>
    <%= f.text_field :state %>
  </div>
  <div>
    <%= f.label :full_address %>
    <%= f.text_field :full_address %>
  </div>
  <div>
    <%= f.label :registration_id %>
    <%= f.text_field :registration_id %>
  </div>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :phone %>
    <%= f.text_field :phone %>
  </div>
  <div>
    <%= f.submit 'Enviar' %>
  </div>
  
<% end %>
